{
  "name": "doai-me-backend",
  "version": "1.0.0",
  "description": "Screen mirroring WebSocket backend service",
  "main": "dist/socket_service.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint src",
    "start": "node dist/socket_service.js",
    "dev": "ts-node socket_service.ts",
    "test:latency": "ts-node latency_test.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "socketio": "node dist/server.js",
    "socketio:dev": "ts-node src/server.ts",
    "worker": "node dist/queue/WorkflowWorker.js",
    "worker:dev": "ts-node src/queue/WorkflowWorker.ts"
  },
  "keywords": [
    "websocket",
    "screen-mirroring",
    "low-latency"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@doai/shared": "file:../../packages/shared",
    "@socket.io/redis-adapter": "^8.3.0",
    "@supabase/supabase-js": "^2.95.3",
    "bullmq": "^5.0.0",
    "cors": "^2.8.6",
    "dotenv": "^16.6.1",
    "express": "^4.21.0",
    "ioredis": "^5.3.2",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "openai": "^6.17.0",
    "socket.io": "^4.8.3",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.0.0",
    "@types/ws": "^8.5.10",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.0",
    "vitest": "^4.0.18"
  }
}
