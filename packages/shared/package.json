{
  "name": "@doai/shared",
  "version": "1.0.0",
  "description": "DoAi.Me 공통 타입, API 스펙, Socket 이벤트 정의",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.js"
    },
    "./api": {
      "types": "./dist/api.d.ts",
      "import": "./dist/api.js",
      "require": "./dist/api.js"
    },
    "./socket": {
      "types": "./dist/socket.d.ts",
      "import": "./dist/socket.js",
      "require": "./dist/socket.js"
    },
    "./queue": {
      "types": "./dist/queue.d.ts",
      "import": "./dist/queue.js",
      "require": "./dist/queue.js"
    },
    "./constants": {
      "types": "./dist/constants.d.ts",
      "import": "./dist/constants.js",
      "require": "./dist/constants.js"
    },
    "./database": {
      "types": "./dist/database.d.ts",
      "import": "./dist/database.js",
      "require": "./dist/database.js"
    },
    "./bot-catalog": {
      "types": "./dist/bot-catalog.d.ts",
      "import": "./dist/bot-catalog.js",
      "require": "./dist/bot-catalog.js"
    },
    "./events": {
      "types": "./dist/events.d.ts",
      "import": "./dist/events.js",
      "require": "./dist/events.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rimraf dist",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.18"
  },
  "typesVersions": {
    "*": {
      "database": [
        "dist/database.d.ts"
      ],
      "types": [
        "dist/types.d.ts"
      ],
      "api": [
        "dist/api.d.ts"
      ],
      "socket": [
        "dist/socket.d.ts"
      ],
      "queue": [
        "dist/queue.d.ts"
      ],
      "constants": [
        "dist/constants.d.ts"
      ],
      "bot-catalog": [
        "dist/bot-catalog.d.ts"
      ],
      "events": [
        "dist/events.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "license": "MIT"
}
