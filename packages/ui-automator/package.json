{
  "name": "@doai/ui-automator",
  "version": "1.0.0",
  "description": "ADB UIAutomator wrapper for Android UI automation",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@doai/worker-core": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.3.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "doai",
    "ui-automator",
    "android",
    "automation",
    "adb"
  ],
  "license": "MIT"
}
